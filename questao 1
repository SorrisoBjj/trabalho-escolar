




#include <stdio.h>



int main() {
    int num;
    // obtendo o tamanho do array

    printf("Digite o tamanho do array: ");
    scanf("%d", &num);

    int A[num];
    // preenchendo o array

    printf("Digite os números inteiros:%d \n", num);
    for (int i = 0; i < num; i++) {
        scanf("%d", &A[i]);
    }
    // imprime o resultado

    int result = somaArray(A, num);

    printf("A soma é: %d\n", result);

    return 0;
}
